{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.5.2",
  "dependencies": {
    "@angular/common": "21.0.0-next.9",
    "@angular/compiler": "21.0.0-next.9",
    "@angular/core": "21.0.0-next.9",
    "@angular/forms": "21.0.0-next.9",
    "@angular/platform-browser": "21.0.0-next.9",
    "@angular/router": "21.0.0-next.9",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular/build": "21.0.0-next.9",
    "@angular/cli": "21.0.0-next.9",
    "@angular/compiler-cli": "21.0.0-next.9",
    "angular-eslint": "20.5.0",
    "eslint": "9.38.0",
    "jsdom": "27.0.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1",
    "vitest": "4.0.5"
  }
}
